    <script>
        const foto = document.querySelector('#fotoInput');
        const imagem = document.querySelector('#foto-texto');
        const imagemTxt = 'Escolha uma foto de perfil';
        imagem.innerHTML = imagemTxt;

        foto.addEventListener('change', (event) => {
            const inputTarget = event.target;
            const file = inputTarget.files[0];

            if (file) {
                const reader = new FileReader();

                reader.addEventListener('load', (event) => {
                    const readerTarget = event.target;

                    const img = document.createElement('img');
                    img.src = readerTarget.result;
                    img.classList.add('foto-em-si');
                    imagem.innerHTML = '';

                    imagem.appendChild(img)
                })

                reader.readAsDataURL(file);
            } else {
                imagem.innerHTML = imagemTxt;
            }
        })
    </script>

